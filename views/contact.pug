extends basic.pug

block scripts
    script(src='../static/script.js')
block style 
    style
        include ../static/style.css
        include ../static/contact.css

block content
    h1 Contact Us
    div(class="container")
      form(action="/contact", method='post',class="flex contact-form")
          input(type="text", name='Name' ,placeholder='Enter Your Name',class='name',autocomplete="off") 
          input(type="email", name='email' ,placeholder='Enter Your Mail ID',class='email',autocomplete="off")
          input(type="phone", name='number' ,placeholder='Enter Your Personal Contact Number',class='number',autocomplete="off")
          input(type="text", name='address' ,placeholder='Enter Your Permananet address',class='address',autocomplete="off")
          input(type="text", name='goal' ,placeholder='Enter Your Goal in details',class='goal',autocomplete="off")
          button(class='btn-primary')
              | Submit

